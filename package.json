{
  "name": "Harper-Grey",
  "version": "1.0.1",
  "description": "Harper Grey Lifestyle main page",
  "main": "index.js",
  "scripts": {
    "start": "node public/server.js",
    "test": "jest",
    "start:dev": "nodemon public/server.js",
    "client:dev": "webpack-dev-server",
    "build": "webpack --config server.webpack.config.js -p",
    "build:dev": "webpack --config server.webpack.config.js -w",
    "build:client": "webpack -p",
    "heroku-postbuild": "npm run build && npm run build:client"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/server/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/__test__/coverage/",
    "resolver": null,
    "testMatch": [
      "**/__test__/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlukenoff/harper-grey.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jlukenoff/harper-grey/issues"
  },
  "homepage": "https://github.com/jlukenoff/harper-grey#readme",
  "dependencies": {
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "express": "^4.16.3",
    "jest": "^23.6.0",
    "merge": "^1.2.1",
    "mongoose": "^5.3.0",
    "next": "^7.0.2",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.12",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "supertest": "^3.3.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-emotion": "^9.2.11",
    "babel-preset-airbnb": "^2.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.0",
    "react-svg-loader": "^2.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
